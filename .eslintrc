{
  "parser": "babel-eslint",
  "env": { "browser": true, "node": true },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "react/prop-types": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    },
    "linkComponents": [{ "name": "Link", "linkAttribute": "to" }]
  }
}
